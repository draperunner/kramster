{
  "name": "kramster",
  "version": "2.0.0",
  "description": "Kramster is a quiz app for making exam preparation more fun",
  "main": "src/server/server.js",
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=.env.staging webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "build:staging": "DOTENV_CONFIG_PATH=.env.staging npm run build",
    "build:prod": "DOTENV_CONFIG_PATH=.env.prod npm run build",
    "prod": "npm run build:prod && NODE_ENV=production node dist/server/server.js",
    "eslint": "eslint --ext js,jsx,ts,tsx src/",
    "stylelint": "stylelint src/**/*.css",
    "lint": "npm run eslint && npm run stylelint",
    "test": "tsc --noEmit",
    "emulators": "firebase emulators:start",
    "deploy": "npm run build:prod && firebase deploy -P prod",
    "deploy:staging": "npm run build:staging && firebase deploy -P staging"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/draperunner/Kramster.git"
  },
  "author": "Mats Byrkjeland",
  "license": "Apache-2.0",
  "dependencies": {
    "chart.js": "^2.9.4",
    "firebase": "^8.6.0",
    "firebase-admin": "^9.8.0",
    "firestore-batcher": "^0.3.1",
    "katex": "^0.11.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-chartjs-2": "^2.11.2",
    "react-dom": "^16.14.0",
    "react-flexbox-grid": "^2.1.2",
    "react-router": "^3.2.6",
    "sanitize-html": "^2.3.2",
    "wilson-score": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/chart.js": "^2.9.32",
    "@types/katex": "^0.11.0",
    "@types/node": "^14.17.33",
    "@types/qs": "^6.9.6",
    "@types/react": "^16.14.6",
    "@types/react-dom": "^16.9.12",
    "@types/react-router": "^3.0.24",
    "@types/sanitize-html": "^1.27.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^4.3.0",
    "firebase-tools": "^9.10.2",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^2.3.0",
    "react-hot-loader": "^4.13.0",
    "react-transform-hmr": "^1.0.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
